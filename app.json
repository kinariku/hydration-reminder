{
  "expo": {
    "name": "StayHydrated",
    "slug": "stayhydrated",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "stayhydrated",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "bundleIdentifier": "app.kinari.stayhydrated",
      "supportsTablet": true,
      "buildNumber": "7",
      "infoPlist": {
        "UIBackgroundModes": [
          "fetch"
        ],
        "NSUserNotificationsUsageDescription": "水分補給のリマインダーを送信するために通知を使用します。",
        "NSCameraUsageDescription": "プロフィール写真を設定するためにカメラを使用します。"
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-sqlite",
      "expo-notifications"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "0a8f8c27-7a9e-41c1-baab-80421e48882c"
      }
    },
    "owner": "kinarikuramoto"
  }
}
